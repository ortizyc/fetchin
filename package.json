{
  "name": "@ortizyc/fetchin-monorepo",
  "private": true,
  "version": "0.0.1",
  "author": "ortizyc <recuritis@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@7.0.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm -y",
    "prepare": "husky install",
    "dev": "pnpm -F !playground-* -r --parallel dev",
    "build": "pnpm -F !playground-* -r build",
    "pg:client": "pnpm -C playgrounds/client dev",
    "pg:server": "pnpm -C playgrounds/server dev",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@ombro/eslint-config-typescript": "^0.1.3",
    "@ombro/tsconfig": "^1.4.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitest/coverage-c8": "^0.22.1",
    "cors": "^2.8.5",
    "eslint": "^8.37.0",
    "execa": "^5.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "minimist": "^1.2.8",
    "prettier": "^2.8.7",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5",
    "vite": "^2.9.15",
    "vite-plugin-dts": "^1.7.3",
    "vitest": "^0.22.1"
  }
}
