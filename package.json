{
  "name": "@ortizyc/fetchin-monorepo",
  "private": true,
  "version": "0.0.1",
  "author": "ortizyc <recuritis@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@7.0.0",
  "engines": {
    "node": ">=14",
    "pnpm": ">=7"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm -y",
    "prepare": "husky install",
    "dev": "pnpm -F !playground-* -r --parallel dev",
    "build": "pnpm -F !playground-* -r build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@ombro/eslint-config-typescript": "^0.1.0",
    "@ombro/tsconfig": "^1.4.2",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitest/coverage-c8": "^0.22.1",
    "cors": "^2.8.5",
    "eslint": "^8.11.0",
    "execa": "^5.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "minimist": "^1.2.6",
    "prettier": "^2.6.1",
    "tsx": "^3.8.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.22.1"
  }
